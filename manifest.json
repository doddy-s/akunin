{
    "manifest_version": 3,
    "name": "AKUNIN",
    "description": "A Chrome Extension for detecting hate speech on X/Twitter using machine learning",
    "version": "0.1",
    "permissions": [
        "https://twitter.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/*"
            ],
            "js": [
                "src/scripts/jquery-3.7.1.min.js",
                "src/scripts/akunin.js"
            ],
            "css": [
                "src/styles/akunin.css"
            ]
        }
    ]
}